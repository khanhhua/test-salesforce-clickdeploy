                </value>
                <value>
                    <fullName>Blacklist</fullName>
                    <default>false</default>
                    <label>Blacklist</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>User__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Used to store the user information for the personal blacklist</description>
        <externalId>false</externalId>
        <label>User</label>
        <referenceTo>User</referenceTo>
        <relationshipName>CandidateList</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Comment__c</fullName>
        <externalId>false</externalId>
        <label>Comment</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ClinicId__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Clinic</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>equals</operation>
                <value>healthcareInstitution</value>
            </filterItems>
            <infoMessage>Only Health Care Institutions are visible</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Candidate Lists (Clinic)</relationshipLabel>
        <relationshipName>ClinicRatings_Candidate</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>UniqueKey__c</fullName>
        <description>Stores the combination of account, person account or account and user id (set by a flow), so that a duplication rule can be set up</description>
        <externalId>false</externalId>
        <label>Unique Key</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CandidateId__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Candidate</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 OR 2</booleanFilter>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>equals</operation>
                <value>doctor</value>
            </filterItems>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>equals</operation>
                <value>caregiver</value>
            </filterItems>
            <infoMessage>Only doctors and care givers are visible in this lookup</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Candidate Lists (Candidate)</relationshipLabel>
        <relationshipName>ClinicRatings_Clinic</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <validationRules>
        <fullName>ClinicOrUserFilled</fullName>
        <active>true</active>
        <description>Hinders saving records where clinic and user is filled</description>
        <errorConditionFormula>NOT(ISBLANK(ClinicId__c )) &amp;&amp; NOT(ISBLANK( User__c))</errorConditionFormula>
        <errorMessage>Please enter either clinic or user.</errorMessage>
    </validationRules>
