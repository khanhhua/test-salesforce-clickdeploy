    <recordTypes>
        <fullName>Campaign_Record_Type</fullName>
        <active>true</active>
        <description>Default Campaign Record Type</description>
        <label>Campaign Record Type</label>
        <picklistValues>
            <picklist>Status</picklist>
            <values>
                <fullName>Closed</fullName>
                <default>false</default>
            </values>
            <values>
