    <fields>
        <fullName>Opportunity__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field is for the relation between Opp and Campaign.
Robin S. 22.02.2019</description>
        <externalId>false</externalId>
        <label>Opportunity</label>
        <lookupFilter>
            <active>true</active>
            <errorMessage>Für diese Opportunity kann keine Kampagne erstellt werden</errorMessage>
            <filterItems>
                <field>Opportunity.RecordTypeId</field>
                <operation>equals</operation>
                <value>Substitute Doctor, Substitute Caregiver, Recruitment Doctor, Recruitment Caregiver</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>Campaigns</relationshipLabel>
        <relationshipName>Campaigns</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Subject_Line__c</fullName>
        <description>This is the subject line for the offer mail.
Robin S. 12.04.2019</description>
        <externalId>false</externalId>
        <label>Subject Line</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>T_Sender_Name__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Opportunity__r.RecordType.DeveloperName = &quot;SubstituteCaregiver&quot;, &quot;doctari - Pflege Jobs&quot;, &quot;doctari - Aerzte Jobs&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>[T] Sender Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Jobportal_Link__c</fullName>
        <externalId>false</externalId>
        <formula>IF(  PrivateCampaign__c,&quot;Der Job ist nicht veröffentlicht&quot;,IF( CONTAINS(Opportunity__r.RecordType.DeveloperName, &quot;Caregiver&quot;), &quot;https://mein.doctari.de/jobs/&quot; &amp; Campaign_ID__c , &quot;https://mein.doctari.de/jobs/&quot; &amp; Campaign_ID__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Jobportal Link</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Operating_Time__c</fullName>
        <description>This field shows the operating time from the opportunity. 
Robin S. 22.08.2019</description>
        <externalId>false</externalId>
        <formula>TEXT(DAY(Opportunity__r.DateFrom__c))&amp;&quot;.&quot;&amp;TEXT(MONTH(Opportunity__r.DateFrom__c))&amp;&quot;.&quot;&amp;TEXT(YEAR(Opportunity__r.DateFrom__c)) &amp; &quot; - &quot; 
&amp; TEXT(DAY(Opportunity__r.DateTo__c))&amp;&quot;.&quot;&amp;TEXT(MONTH(Opportunity__r.DateTo__c))&amp;&quot;.&quot;&amp;TEXT(YEAR(Opportunity__r.DateTo__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Die Einsatzzeit entspricht der Zeit, die in der Opportunity festgelegt wurde.</inlineHelpText>
        <label>Operating Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>T_MCAPICallout__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>[T] MCAPICallout</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Owner_Full_Name__c</fullName>
        <description>This formula Field is used for a List View on the PersonAccount Layout.
Robin S. 24.05.2019</description>
        <externalId>false</externalId>
        <formula>Opportunity__r.Owner.FirstName + &quot; &quot; + Opportunity__r.Owner.LastName</formula>
