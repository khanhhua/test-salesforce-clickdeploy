                <fullName>VAngebot</fullName>
                <default>true</default>
            </values>
        </picklistValues>
    </recordTypes>
    <recordTypes>
        <fullName>Campaign_Sent_Closed_Record_Type</fullName>
        <active>true</active>
        <description>The Campaign is read-only in status &quot;Sent&quot; or &quot;Closed&quot;</description>
        <label>Campaign (Sent/Closed) Record Type</label>
        <picklistValues>
            <picklist>Status</picklist>
            <values>
                <fullName>Closed</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Sent</fullName>
                <default>false</default>
            </values>
        </picklistValues>
        <picklistValues>
            <picklist>Type</picklist>
            <values>
                <fullName>VAngebot</fullName>
                <default>true</default>
            </values>
        </picklistValues>
    </recordTypes>
    <validationRules>
        <fullName>CampaignIsReadOnlyInStatusClosedOrSent</fullName>
        <active>false</active>
        <description>In Status &quot;Sent&quot; or &quot;Closed&quot; the Campaign is read only.</description>
        <errorConditionFormula>Not(ISCHANGED(  MarketoAPICallout__c  )) &amp;&amp;
(ISCHANGED( Status ) &amp;&amp;
 
NOT(ISPICKVAL(PRIORVALUE(Status), &apos;In_Preparation&apos;)) &amp;&amp;
ISPICKVAL(Status, &apos;In_Preparation&apos;)

) || 
NOT(ISCHANGED(Status)) &amp;&amp; NOT(ISPICKVAL(Status, &apos;In_Preparation&apos;))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>In Status &quot;Sent&quot; or &quot;Closed&quot; the Campaign is read only.</errorMessage>
    </validationRules>
