    <validationRules>
        <fullName>QuoteRequired</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(Contract__r.RecordType.Name &lt;&gt; &apos;Porky (History)&apos;, ISBLANK(Quote__c )), true, false)</errorConditionFormula>
        <errorMessage>Der Vertragsentwurf muss ausgefüllt sein</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockOperationTimes</fullName>
        <active>true</active>
        <description>Hinders users from editing operation times, where contract is not in a draft status anymore.</description>
        <errorConditionFormula>NOT(
				ISBLANK(Contract__c) ||
				(NOT(ISBLANK(Contract__c)) &amp;&amp; ISPICKVAL(Contract__r.Status, &quot;Draft&quot;)) ||
				ISCHANGED(Contract__c) ||
				$Permission.EditFinalContracts
				)</errorConditionFormula>
        <errorMessage>This operation time can not be edited any more. It&apos;s contract has already exceeded the draft status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MindeststundenRequired</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(Contract__r.RecordType.Name &lt;&gt; &apos;Porky (History)&apos;, ISBLANK(MonthlyHoursCustomer__c)), true, false)</errorConditionFormula>
        <errorMessage>Die Mindeststunden müssen ausgefüllt sein</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>From_less_or_equal_than_To</fullName>
        <active>true</active>
        <errorConditionFormula>From__c  &gt;  To__c</errorConditionFormula>
        <errorDisplayField>From__c</errorDisplayField>
        <errorMessage>&quot;Von&quot; muss das gleiche oder ein früheres Datum als &quot;Bis&quot; beinhalten.</errorMessage>
    </validationRules>
</CustomObject>