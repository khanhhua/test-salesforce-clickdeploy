    <fields>
        <fullName>T_Operation_Time_CM__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
( MONTH(From__c) &gt;= MONTH(TODAY()) &amp;&amp; YEAR(From__c) = YEAR(TODAY()) &amp;&amp;
  MONTH(To__c)   &lt;= MONTH(TODAY()) &amp;&amp; YEAR(To__c)   = YEAR(TODAY()) )

||

( MONTH(From__c) &lt;= MONTH(TODAY()) &amp;&amp; YEAR(From__c) = YEAR(TODAY()) &amp;&amp;
  MONTH(To__c)   &gt;= MONTH(TODAY()) &amp;&amp; YEAR(To__c)   = YEAR(TODAY()) ),

1,
0)</formula>
        <label>[T] Operation Time CM</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
